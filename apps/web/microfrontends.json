{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "web": {
      "development": {
        "local": 3002,
        "fallback": "http://localhost:3000"
      }
    },
    "helpdesk": {
      "development": {
        "local": 3003
      },
      "routing": [
        {
          "group": "helpdesk",
          "paths": ["/helpdesk/:path*"]
        }
      ]
    }
  },
  "options": {
    "localProxyPort": 3000
  }
}
